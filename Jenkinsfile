
pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "Hello World"
            }
        }
        stage('Test') {
            steps {
                echo "Going to be tested"
            }
        }
        stage('Deploy') {
            steps {
                echo "Going to be Deploy"
            }
        }
        stage('Production') {
            steps {
                echo "Going to be Production"
            }
        }
    }
}
